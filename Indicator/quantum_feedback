    <script>
        // Quantum Circuit Visualization
        const circuitCtx = document.getElementById('quantumCircuit').getContext('2d');
        
        // Create quantum circuit visualization
        new Chart(circuitCtx, {
            type: 'bar',
            data: {
                labels: ['Q0', 'Q1', 'Q2', 'Q3', 'Q4'],
                datasets: [{
                    label: 'Hadamard Gates',
                    data: [1, 1, 1, 1, 1],
                    backgroundColor: '#8A2BE2',
                    barThickness: 15
                }, {
                    label: 'Phase Gates',
                    data: [0, 1, 2, 3, 4],
                    backgroundColor: '#00FFFF',
                    barThickness: 15
                }, {
                    label: 'CNOT Gates',
                    data: [2, 2, 1, 1, 0],
                    backgroundColor: '#FFD700',
                    barThickness: 15
                }]
            },
            options: {
                indexAxis: 'y',
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    x: {
                        stacked: true,
                        grid: {
                            color: 'rgba(255, 255, 255, 0.1)'
                        },
                        ticks: {
                            color: '#FFFFFF'
                        }
                    },
                    y: {
                        stacked: true,
                        grid: {
                            color: 'rgba(255, 255, 255, 0.1)'
                        },
                        ticks: {
                            color: '#FFFFFF'
                        }
                    }
                },
                plugins: {
                    legend: {
                        labels: {
                            color: '#FFFFFF'
                        }
                    },
                    title: {
                        display: false
                    }
                }
            }
        });
        
        // Performance Comparison Chart
        const perfCtx = document.getElementById('performanceChart').getContext('2d');
        
        new Chart(perfCtx, {
            type: 'bar',
            data: {
                labels: ['Frequency Analysis', 'Pattern Recognition', 'Parameter Optimization', 'Overall'],
                datasets: [{
                    label: 'Classical System',
                    data: [1.0, 1.0, 1.0, 1.0],
                    backgroundColor: 'rgba(255, 255, 255, 0.6)',
                    borderColor: 'rgba(255, 255, 255, 0.8)',
                    borderWidth: 1
                }, {
                    label: 'Quantum System',
                    data: [3.1, 1.8, 2.0, 2.3],
                    backgroundColor: 'rgba(0, 255, 255, 0.6)',
                    borderColor: 'rgba(0, 255, 255, 0.8)',
                    borderWidth: 1
                }]
            },
            options: {
                responsive: true,
                maintainAspectRatio: false,
                scales: {
                    x: {
                        grid: {
                            color: 'rgba(255, 255, 255, 0.1)'
                        },
                        ticks: {
                            color: '#FFFFFF'
                        }
                    },
                    y: {
                        grid: {
                            color: 'rgba(255, 255, 255, 0.1)'
                        },
                        ticks: {
                            color: '#FFFFFF'
                        },
                        title: {
                            display: true,
                            text: 'Performance Multiplier',
                            color: '#FFFFFF'
                        }
                    }
                },
                plugins: {
                    legend: {
                        labels: {
                            color: '#FFFFFF'
                        }
                    },
                    title: {
                        display: false
                    }
                }
            }
        });
    </script>
